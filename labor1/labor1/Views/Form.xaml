<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:views="clr-namespace:labor1.Views"
             x:Class="labor1.Views.Form">
    <ContentPage.Content>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>

            <views:CustomForm Grid.Row="0" Grid.Column="0" />
            <views:CustomForm Grid.Row="0" Grid.Column="1" />
            <views:CustomForm Grid.Row="1" Grid.Column="0" />
            <views:CustomForm Grid.Row="1" Grid.Column="1" />
        </Grid>

    </ContentPage.Content>
</ContentPage>